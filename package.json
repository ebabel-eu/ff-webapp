{
  "name": "ff-webapp",
  "version": "1.0.0",
  "description": "Fighting Fantasy Web App with React & Redux",
  "main": "webpack.config.js",
  "dependencies": {
    "babel-loader": "6.2.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-html-attrs": "2.0.0",
    "babel-plugin-transform-class-properties": "6.3.13",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "copy-webpack-plugin": "^2.1.3",
    "history": "1.17.0",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-redux": "4.4.0",
    "react-router": "1.0.3",
    "redux": "3.3.1",
    "webpack": "1.13.0"
  },
  "scripts": {
    "postinstall": "node ./node_modules/webpack/bin/webpack.js -p --config ./webpack.config.js --progress --colors",
    "start": "node index.js",
    "test": "karma start --single-run",
    "text-watch": "karma start"
  },
  "author": "Didier Chartrain",
  "license": "MIT",
  "semistandard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "react-html-attrs",
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^1.0.2",
    "eslint-plugin-react": "^5.0.1",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22"
  }
}
